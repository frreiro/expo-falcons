{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "module": "es6",
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@app/*": ["src/app/*"],
      "@components/*": ["src/app/components/*"],
      "@layouts/*": ["src/app/layouts/*"],
      "@pages/*": ["src/app/pages/*"],
      "@providers/*": ["src/app/providers/*"],
      "@globalStyle/*": ["src/styles/*"],
      "@redux/*": ["src/redux/*"],
      "@resource/*": ["resource/*"]
    }
  }
}
