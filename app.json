{
  "expo": {
    "name": "Falcons",
    "slug": "Falcons",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/custom/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/custom/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#292424"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ufformula.falcons",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/custom/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
